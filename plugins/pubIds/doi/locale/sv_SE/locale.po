msgstr "DOI:n kan inte tilldelas eftersom det anpassade suffixet saknas."

msgid "plugins.pubIds.doi.editor.assignDoi"
msgstr "Tilldela DOI:n {$pubId} till denna {$pubObjectType}"

msgid "plugins.pubIds.doi.editor.clearObjectsDoi.confirm"
msgstr "Är du säker på att du vill radera den nuvarande DOI:n?"

msgid "plugins.pubIds.doi.editor.clearObjectsDoi"
msgstr "Rensa"

msgid "plugins.pubIds.doi.editor.doiSuffixCustomIdentifierNotUnique"
msgstr ""
"Det givna DOI-suffixet används redan i ett annat objekt. Ange ett unikt DOI-"
"suffix för varje objekt."

msgid "plugins.pubIds.doi.editor.assigned"
msgstr "DOI:n är tilldelad till denna {$pubObjectType}."

msgid "plugins.pubIds.doi.editor.canBeAssigned"
msgstr ""
"Du ser nu en förhandsvisning av DOI:n. Kryssa i rutan och spara formuläret "
"för att tilldela DOI:n."

msgid "plugins.pubIds.doi.editor.patternNotResolved"
msgstr "DOI kan inte tilldelas eftersom den innehåller ett olösbart mönster."

msgid "plugins.pubIds.doi.editor.missingParts"
msgstr ""
"DOI kan inte genereras eftersom en eller flera delar av DOI-mönstret saknar "
"data."

msgid "plugins.pubIds.doi.editor.customSuffixMissing"
msgstr "DOI kan inte tilldelas eftersom det anpassade suffixet saknas."

msgid "plugins.pubIds.doi.editor.doiObjectTypeSubmissionFile"
msgstr "fil"

msgid "plugins.pubIds.doi.editor.doiObjectTypeRepresentation"
msgstr "publiceringsformat"

msgid "plugins.pubIds.doi.editor.doiObjectTypeChapter"
msgstr "kapitel"

msgid "plugins.pubIds.doi.editor.doiObjectTypeSubmission"
msgstr "bok"

